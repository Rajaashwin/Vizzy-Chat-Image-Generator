{
  "buildCommand": "pip install -r backend/requirements.txt",
  "env": {
    "OPENROUTER_API_KEY": "@openrouter_api_key",
    "HUGGINGFACE_API_KEY": "@huggingface_api_key",
    "REPLICATE_API_KEY": "@replicate_api_key",
    "ALLOWED_ORIGINS": "@allowed_origins"
  },
  "functions": {
    "api/index.py": {
      "runtime": "python3.11",
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ]
}
